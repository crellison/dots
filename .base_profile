export  PATH=$HOME/bin:/usr/local/bin:$PATH
export  PATH=$PATH:/usr/local/go/bin
export  DOTS=$HOME/dotfiles
export  PATH="/usr/local/sbin:$PATH"

# readline
export  LDFLAGS="-L/usr/local/opt/readline/lib"
export  CPPFLAGS="-I/usr/local/opt/readline/include"

# nvm
export NVM_DIR="$HOME/.nvm"
. "/usr/local/opt/nvm/nvm.sh"

# work
source  $DOTS/.work

# pyenv setup
if command -v pyenv 1>/dev/null 2>&1; then
  eval "$(pyenv init -)"
fi
export PYTHONSTARTUP=$DOTS/.pythonrc

# java
export JAVA_HOME=/Library/Java/JavaVirtualMachines/jdk1.8.0_221.jdk/Contents/Home
export PATH=/opt/apache-maven-3.6.0/bin:$PATH

# go
export PATH=$PATH:$(go env GOPATH)/bin

# kubernetes
source  $DOTS/.kubernetes

# good stuff
source  $DOTS/.aliases
source  $DOTS/.functions

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/cellison/dev/google-cloud-sdk/path.zsh.inc' ]; then . '/Users/cellison/dev/google-cloud-sdk/path.zsh.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/cellison/dev/google-cloud-sdk/completion.zsh.inc' ]; then . '/Users/cellison/dev/google-cloud-sdk/completion.zsh.inc'; fi
